# RunPod Configuration
# Copy this file to .env and fill in your actual values

# Your RunPod API key from https://www.runpod.io/console
RUNPOD_API_KEY=your_runpod_api_key_here

# Your RunPod serverless endpoint ID for text-to-image
# Deploy a text-to-image model (like Stable Diffusion) and get the endpoint ID
# Just the endpoint ID, not the full URL!
RUNPOD_TEXT_TO_IMAGE_ENDPOINT=your_endpoint_id_here

# Upstash Redis Configuration (for queue system)
UPSTASH_REDIS_REST_TOKEN=your_upstash_token_here
UPSTASH_REDIS_REST_URL=your_upstash_url_here

# Optional: Add more endpoints as you deploy them
# RUNPOD_IMAGE_TO_IMAGE_ENDPOINT=your_image_to_image_endpoint_id_here
# RUNPOD_TEXT_TO_VIDEO_ENDPOINT=your_text_to_video_endpoint_id_here
